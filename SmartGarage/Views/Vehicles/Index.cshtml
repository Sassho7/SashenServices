@model IList<SmartGarage.ViewModels.VehicleViewModel>

@{
    ViewData["Title"] = "Vehicle List";
}

<h1>@ViewData["Title"]</h1>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>License Plate</th>
            <th>VIN</th>
            <th>Model</th>
            <th>Brand</th>
            <th>Creation Year</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var vehicle in Model)
        {
            <tr>
                <td>@vehicle.Id</td>
                <td>@vehicle.LicensePlate</td>
                <td>@vehicle.VIN</td>
                <td>@vehicle.Model</td>
                <td>@vehicle.Brand</td>
                <td>@vehicle.CreationYear</td>
            </tr>
        }
    </tbody>
</table>
